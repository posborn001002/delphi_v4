<p id="notice"><%= notice %></p>

<h1>Updates</h1>

<table>
  <thead>
    <tr>
      <th>Datelookup</th>
      <th>Person</th>
      <th>Actionresult</th>
      <th>Job order</th>
      <th>Job order type</th>
      <th>Attachment</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @updates.each do |update| %>
      <tr>
        <td><%= update.DateLookup %></td>
        <td><%= update.Person %></td>
        <td><%= update.ActionResult %></td>
        <td><%= update.job_order_id %></td>
        <td><%= update.job_order_type %></td>
        <td><%= update.attachment %></td>
        <td><%= update.text %></td>
        <td><%= link_to 'Show', update %></td>
        <td><%= link_to 'Edit', edit_update_path(update) %></td>
        <td><%= link_to 'Destroy', update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Update', new_update_path %>
