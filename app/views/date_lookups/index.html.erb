<p id="notice"><%= notice %></p>

<h1>Date Lookups</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Day of month</th>
      <th>Month</th>
      <th>Quarter</th>
      <th>Year</th>
      <th>Week of year</th>
      <th>Week of quarter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_lookups.each do |date_lookup| %>
      <tr>
        <td><%= date_lookup.date %></td>
        <td><%= date_lookup.day_of_month %></td>
        <td><%= date_lookup.month %></td>
        <td><%= date_lookup.quarter %></td>
        <td><%= date_lookup.year %></td>
        <td><%= date_lookup.week_of_year %></td>
        <td><%= date_lookup.week_of_quarter %></td>
        <td><%= link_to 'Show', date_lookup %></td>
        <td><%= link_to 'Edit', edit_date_lookup_path(date_lookup) %></td>
        <td><%= link_to 'Destroy', date_lookup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date Lookup', new_date_lookup_path %>
