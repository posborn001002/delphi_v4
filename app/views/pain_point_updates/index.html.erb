<p id="notice"><%= notice %></p>

<h1>Pain Point Updates</h1>

<table>
  <thead>
    <tr>
      <th>Painpoint</th>
      <th>Progress</th>
      <th>Progress type</th>
      <th>Pass fail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pain_point_updates.each do |pain_point_update| %>
      <tr>
        <td><%= pain_point_update.PainPoint %></td>
        <td><%= pain_point_update.progress_id %></td>
        <td><%= pain_point_update.progress_type %></td>
        <td><%= pain_point_update.pass_fail %></td>
        <td><%= link_to 'Show', pain_point_update %></td>
        <td><%= link_to 'Edit', edit_pain_point_update_path(pain_point_update) %></td>
        <td><%= link_to 'Destroy', pain_point_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pain Point Update', new_pain_point_update_path %>
